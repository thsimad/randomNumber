<%- include partials/header %>
<div class="container">
    <div class="applicantProfile">
        <div class="formP formB">
                <%if(data.linkedinUrl === undefined){%>
                    <%if(data.fblink === undefined){%>

                    <%}else{%>
                        <br><a class="applicantLinks" target="_blank" href="<%=data.fblink%>"><i class="fab fa-facebook"></i></a>
                    <%}%>
                <%}else{%>
                    <a class="applicantLinks" target="_blank" href="<%=data.linkedinUrl%>"><i class="fab fa-linkedin"></i></a>
                <%}%>
                <%if(user){%>
                    <div class="profile prof">
                        <img class="profileImage" src="<%=data.linkedinProfileUrl%>" onerror=this.src="https://i.imgur.com/V0ZBBeO.png" alt="<%=data.name%>">
                        <h4><%=data.name%></h4>
                        <h6><%=data.linkedinHeadline%></h6>
                    </div>
                <%}else{%>
                <%}%>
            <div class="form-group">
                <label class="rg-from" for="usr" >Name*</label>
                <h4><%=data.name%></h4>
            </div>
            <div class="form-group">
                <label class="rg-from" for="usr">Email*</label>
                <h4><%=data.email%></h4>    
            </div>
            <div class="form-group">
                <label class="rg-from" for="usr">Mobile*</label>
                <h4><%=data.mobile%></h4>                    
                
            </div>
            <div class="form-group">
                <label class="rg-from" for="usr">Where do you stay?*</label>
                <h4><%=data.location%></h4>
            </div>
            <div class="form-group">
                <label class="rg-from" for="usr">What do you currently do?*</label>
                <h4><%=data.designation%></h4>                
            </div>
            <div class="form-group">
                <label class="rg-from" for="usr">Do you have background in coding?*</label>
                <h4><%=data.codingBackground%></h4>                
            </div>
            <div class="form-group">
                <label class="rg-from" for="usr">Your objective of joining our bootcamp*</label>
                <h4><%=data.joiningObjective%></h4>
            </div>
            <div class="form-group">
                <label class="rg-from" for="usr">Which batch are you looking to join?*</label>
                <h4><%=data.batchType%></h4>
            </div>
            <div class="form-group">
                <label class="rg-from" for="usr">How did you hear about us?*</label>
                <h4><%=data.source%></h4>
            </div>
            <div class="form-group">
                <label class="rg-from" for="usr">What is your expectation from the program?*</label>
                <h4><%=data.expectations%></h4>
            </div>
            <div class="form-group">
                <label class="rg-from" for="usr">Ask for new Recommendations?</label>
                <h4><%=data.recommendation%></h4>      
            </div>
            <div class="form-group">
                <label class="rg-from" for="usr">Need financial assistance?</label>
                <h4><%=data.finassistance%></h4>
            </div>
        </div>
    </div>
</div>

<%- include partials/footer %>
